{
  "name": "studiobear-site",
  "description": "Studiobear website",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Barry Low <barry@studiobear.co>",
  "license": "MIT",
  "scripts": {
    "develop": "run-p --race dev cy:open",
    "dev": "sapper dev",
    "debug": "ndb yarn dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "start:export": "npx serve __sapper__/export",
    "eslint": "eslint --ext .cjs,.js,.ts,.json ./src",
    "eslint:fix": "eslint --fix --ext .cjs,.js,.ts,.json ./src",
    "lint": "prettier --check 'src/**/*.{html,js,svelte}'",
    "lint:fix": "prettier --write 'src/**/*.{html,js,svelte}'",
    "jest": "jest src",
    "jest:watch": "yarn jest --watch src",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race jest",
    "kill": "kill $(lsof -ti:3001,3000,3002)"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "1.0.0-next.11",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@studiobear/designspek": "^0.4.2",
    "@studiobear/designspek-components": "^0.4.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^26.3.0",
    "cypress": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^25.1.0",
    "jest-transform-svelte": "^2.1.1",
    "ndb": "^1.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.1",
    "prettier-plugin-svelte": "^1.2.1",
    "rehype-stringify": "^8.0.0",
    "remark": "^12.0.1",
    "remark-excerpt": "^1.0.0-beta.1",
    "remark-frontmatter": "^2.0.0",
    "remark-html": "^12.0.0",
    "remark-parse": "^8.0.3",
    "remark-parse-yaml": "^0.0.3",
    "remark-rehype": "^7.0.0",
    "rollup": "^2.26.10",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.4",
    "svelte": "^3.24.1",
    "svelte-jester": "^1.1.5",
    "to-vfile": "^6.0.0",
    "unified": "^9.2.0",
    "unified-stream": "^1.0.6",
