{
  "name": "list-baby",
  "version": "0.0.1",
  "browser": "true",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "dev:livereload": "cross-env LIVERELOAD=1 rollup -c -w",
    "dev:nollup": "cross-env NOLLUP=1 nollup -c --hot --port 5000 --content-base ./public",
    "start": "sirv public",
    "clean": "rm -rf ./public/build",
    "eslint": "eslint --ext .cjs,.js,.ts,.json ./src",
    "eslint:fix": "eslint --fix --ext .cjs,.js,.ts,.json ./src",
    "lint": "prettier --check 'src/**/*.{html,js,svelte}'",
    "lint:fix": "prettier --write 'src/**/*.{html,js,svelte}'",
    "jest": "jest src",
    "jest:watch": "yarn jest --watch src",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race jest"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "@studiobear/designspek": "^0.4.1",
    "@studiobear/designspek-components": "^0.4.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/svelte": "^1.11.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.0",
    "cypress": "^4.12.1",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^25.1.0",
    "jest-transform-svelte": "^2.1.1",
    "nollup": "^0.10.6",
    "prettier": "2.1.1",
    "prettier-plugin-svelte": "^1.2.1",
    "rollup": "^1.21.4",
    "rollup-plugin-hot": "^0.0.11",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-svelte-hot": "^0.10.0",
    "rollup-plugin-terser": "^5.2.0",
    "svelte": "^3.24.1",
    "typography-theme-kirkham": "^0.16.19"
  },
  "dependencies": {
    "nanoid": "^2.1.11",
    "sirv-cli": "^0.4.5"
  },
  "peerDependencies": {
    "cypress": "^4.0.1",
    "svelte": "^3.19.1"
  }
}
